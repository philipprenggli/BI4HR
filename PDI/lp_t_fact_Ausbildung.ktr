    <connection>Pentaho_Staging</connection>
    <sql>SELECT    
    EMC.EMPKEY
  , REG.REGKEY
  , REG.EVGKEY
  , REG.EVTKEY

  , REG.REGSTAT AS TeilnahmestatusEnumerator
  , REG.RUSKEY AS Teilnahmestatus2Enumerator
  --, REG.REGWLPOS AS REG_WAITING_LIST_POSITION
  --, REG.REGCORR AS REG_CORRESPONDENT
  , REG.REGDREG AS Anmeldedatum
  , REG.REGDOUT AS Abmeldedatum
  --, REG.BUIKEY AS REG_HOTEL_KEY
  , REG.REGDARR AS Anreisedatum
  , REG.REGDDEP AS Abreisedatum
  , REG.REGHSTS AS HotelstatusEnumerator
  --Bei Umbuchung Anmeldung Anzahl auf 0 setzen
  ,	CASE
		WHEN REG.REGSTAT = &apos;C&apos; THEN 0
		ELSE REG.REGNREG
		END AS AnmeldungAnzahl

  --, REG.REGAT AS REG_ALLOCATION_TYPE
  --, REG.REGFIBUC AS REG_FIBU_COMPANY
  --, REG.REGCC AS REG_COST_CENTER_ENUMERATOR
  --, REG.REGPRCTY AS REG_PRICE_TYPE_SURROGATE
  --, REG.REGPRCCU AS REG_PRICE_CURRENCY_CODE
  --, REG.REGPRCDA AS REG_PRICE_VALID_FROM
  --, REG.REGREM AS REG_REMARK
  --, REG.SEMKEY
  --, REG.ECOKEY
  --, REG.REGCOMDAT AS REG_COMMITMENT_DATE
  --, REG.REGTYPE AS REG_EVENT_TYPE
  --, REG.REGTYPER AS REG_TYPE
  --, REG.REGGRP AS REG_GROUP_OF_REGISTRATION_REG_KEY
  --, REG.REGIRC AS REG_SETTLEMENT_LEVEL
  --, REG.REGDRC AS REG_DOCUMENT_RECIPIENT_LEVEL
  --, REG.RPRKEY 
  --, REG.REGEXPD AS REG_EXPIRY_DATE
  --, REG.REGCOND AS REG_CONFIRMATION_DATE
  --, REG.REGINVD AS REG_INVOICE_DATE
  --, REG.REGUADM AS REG_USERID_ADMINISTRATOR
  --, REG.REGRES1 AS REG_RESERVE_STATUS
    
	--Bei Umbuchung Anlass Kurstage auf 0 setzen
  ,	CASE
		WHEN REG.REGSTAT = &apos;C&apos; THEN 0
		ELSE EVT.EVTDAYS
		END AS AnlassKurstage

FROM lp_t_stage_HRMREG REG
LEFT OUTER JOIN lp_t_stage_HRMEMC EMC ON REG.REGMASCO = EMC.PADMACON AND EMC.EMCWACA = &apos;Y&apos;

LEFT OUTER JOIN lp_t_stage_HRMEVT EVT ON REG.EVTKEY = EVT.EVTKEY



WHERE NOT EMPKEY IS NULL --Registrationen mit leerem EMPKEY sollten wahrscheinlich nicht sein?


ORDER BY EMPKEY, REGKEY</sql>
