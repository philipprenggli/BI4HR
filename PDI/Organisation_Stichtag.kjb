    <entry>
      <name>Rekursive SQL Abfrage</name>
      <description/>
      <type>SQL</type>
      <sql>SET ANSI_NULLS ON
;
SET QUOTED_IDENTIFIER ON
;
SET ANSI_PADDING ON
;
IF OBJECT_ID(&apos;tempdb..#ORG&apos;) IS NOT NULL
DROP TABLE #ORG
;
IF OBJECT_ID(&apos;tempdb..#ORG2&apos;) IS NOT NULL
DROP TABLE #ORG2
;
WITH RecursiveCTE
As 
( SELECT  OUNDESC
 , p.ORGCHDOU
 , CAST(OUN.OUNDESC AS VARCHAR(MAX)) AS TreePath
 , CAST(CASE WHEN OUN.OLVCODE = &apos;&apos; THEN RTRIM(OUN.OUNTYPE) ELSE OUN.OLVCODE END AS VARCHAR(MAX)) AS OLVCODE
 , CAST(OUNTYPE AS VARCHAR(MAX)) AS OUNTYPE
 , ORGPNTOU  
 FROM lp_t_stage_HRMORG p 
 INNER JOIN lp_t_stage_HRMOUN OUN ON
	P.ORGCHDOU = OUN.OUNKEY
 WHERE p.ORGPNTOU = 0 AND CONVERT(${DB_DATEFORMAT},&apos;${Stichtag}&apos;,${DB_DATESTYLE}) BETWEEN ORGFROM AND ORGUNTIL  AND CONVERT(${DB_DATEFORMAT},&apos;${Stichtag}&apos;,${DB_DATESTYLE}) BETWEEN OUNFROM AND OUNUNTIL AND ORSID = ${XHRM_ORSID}
 UNION ALL
 SELECT OUN.OUNDESC
  , p.ORGCHDOU
  , CAST(H.TreePath + &apos;,&apos;
   + CAST(OUN.OUNDESC AS VARCHAR(100)) AS VARCHAR(MAX))
   AS TreePath
   , CAST(H.OLVCODE + &apos;,&apos;
   + CAST(CASE WHEN OUN.OLVCODE = &apos;&apos; THEN RTRIM(OUN.OUNTYPE) ELSE OUN.OLVCODE END AS VARCHAR(100)) AS VARCHAR(MAX)) AS OLVCODE
   , CAST(H.OUNTYPE + &apos;,&apos; 
   + CAST(OUN.OUNTYPE AS VARCHAR(MAX)) AS VARCHAR(MAX)) AS OUNTYPE
  , p.ORGPNTOU  
 FROM lp_t_stage_HRMORG p 
  INNER JOIN lp_t_stage_HRMOUN OUN ON
	P.ORGCHDOU = OUN.OUNKEY
 INNER JOIN RecursiveCTE H
 ON H.ORGCHDOU = p.ORGPNTOU  
  WHERE CONVERT(${DB_DATEFORMAT},&apos;${Stichtag}&apos;,${DB_DATESTYLE}) BETWEEN ORGFROM AND ORGUNTIL AND CONVERT(${DB_DATEFORMAT},&apos;${Stichtag}&apos;,${DB_DATESTYLE}) BETWEEN OUNFROM AND OUNUNTIL AND ORSID = ${XHRM_ORSID}
)  
SELECT OUNDESC
 , ORGCHDOU
 , ORGPNTOU 
 , TreePath
 , OLVCODE
 , OUNTYPE
INTO #ORG
FROM RecursiveCTE




;
WITH CTE AS (
SELECT ORGCHDOU, OUNDESC
 ,CONVERT(XML,&apos;&lt;OE&gt;&lt;Lvl&gt;&apos;
        + REPLACE(REPLACE([TreePath],&apos;&amp;&apos;,&apos;&amp;amp;&apos;),&apos;,&apos;, &apos;&lt;&#47;Lvl&gt;&lt;Lvl&gt;&apos;) + &apos;&lt;&#47;Lvl&gt;&lt;&#47;OE&gt;&apos;)
 AS OE_Hierarchy
  ,CONVERT(XML,&apos;&lt;Ebene&gt;&lt;Lvl&gt;&apos;
        + REPLACE(REPLACE([OLVCODE],&apos;&amp;&apos;,&apos;&amp;amp;&apos;),&apos;,&apos;, &apos;&lt;&#47;Lvl&gt;&lt;Lvl&gt;&apos;) + &apos;&lt;&#47;Lvl&gt;&lt;&#47;Ebene&gt;&apos;)
 AS Ebene_Hierarchy
  ,CONVERT(XML,&apos;&lt;Typ&gt;&lt;Lvl&gt;&apos;
        + REPLACE(REPLACE([OUNTYPE],&apos;&amp;&apos;,&apos;&amp;amp;&apos;),&apos;,&apos;, &apos;&lt;&#47;Lvl&gt;&lt;Lvl&gt;&apos;) + &apos;&lt;&#47;Lvl&gt;&lt;&#47;Typ&gt;&apos;)
 AS Typ_Hierarchy
FROM #ORG a

)
, EbeneBreakdown AS (
SELECT    ORGCHDOU
 , OUNDESC
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[1]&apos;,&apos;varchar(100)&apos;) AS [OE1]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[2]&apos;,&apos;varchar(100)&apos;) AS [OE2]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[3]&apos;,&apos;varchar(100)&apos;) AS [OE3]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[4]&apos;,&apos;varchar(100)&apos;) AS [OE4]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[5]&apos;,&apos;varchar(100)&apos;) AS [OE5] 
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[6]&apos;,&apos;varchar(100)&apos;) AS [OE6]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[7]&apos;,&apos;varchar(100)&apos;) AS [OE7]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[8]&apos;,&apos;varchar(100)&apos;) AS [OE8]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[9]&apos;,&apos;varchar(100)&apos;) AS [OE9]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[10]&apos;,&apos;varchar(100)&apos;) AS [OE10]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[11]&apos;,&apos;varchar(100)&apos;) AS [OE11]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[12]&apos;,&apos;varchar(100)&apos;) AS [OE12]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[13]&apos;,&apos;varchar(100)&apos;) AS [OE13]
 , OE_Hierarchy.value(&apos;&#47;OE[1]&#47;Lvl[14]&apos;,&apos;varchar(100)&apos;) AS [OE14]
 
  , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[1]&apos;,&apos;varchar(100)&apos;) AS [Ebene1]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[2]&apos;,&apos;varchar(100)&apos;) AS [Ebene2]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[3]&apos;,&apos;varchar(100)&apos;) AS [Ebene3]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[4]&apos;,&apos;varchar(100)&apos;) AS [Ebene4]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[5]&apos;,&apos;varchar(100)&apos;) AS [Ebene5] 
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[6]&apos;,&apos;varchar(100)&apos;) AS [Ebene6]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[7]&apos;,&apos;varchar(100)&apos;) AS [Ebene7]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[8]&apos;,&apos;varchar(100)&apos;) AS [Ebene8]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[9]&apos;,&apos;varchar(100)&apos;) AS [Ebene9]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[10]&apos;,&apos;varchar(100)&apos;) AS [Ebene10]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[11]&apos;,&apos;varchar(100)&apos;) AS [Ebene11]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[12]&apos;,&apos;varchar(100)&apos;) AS [Ebene12]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[13]&apos;,&apos;varchar(100)&apos;) AS [Ebene13]
 , Ebene_Hierarchy.value(&apos;&#47;Ebene[1]&#47;Lvl[14]&apos;,&apos;varchar(100)&apos;) AS [Ebene14]
 
   , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[1]&apos;,&apos;varchar(100)&apos;) AS [Typ1]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[2]&apos;,&apos;varchar(100)&apos;) AS [Typ2]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[3]&apos;,&apos;varchar(100)&apos;) AS [Typ3]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[4]&apos;,&apos;varchar(100)&apos;) AS [Typ4]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[5]&apos;,&apos;varchar(100)&apos;) AS [Typ5] 
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[6]&apos;,&apos;varchar(100)&apos;) AS [Typ6]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[7]&apos;,&apos;varchar(100)&apos;) AS [Typ7]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[8]&apos;,&apos;varchar(100)&apos;) AS [Typ8]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[9]&apos;,&apos;varchar(100)&apos;) AS [Typ9]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[10]&apos;,&apos;varchar(100)&apos;) AS [Typ10]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[11]&apos;,&apos;varchar(100)&apos;) AS [Typ11]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[12]&apos;,&apos;varchar(100)&apos;) AS [Typ12]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[13]&apos;,&apos;varchar(100)&apos;) AS [Typ13]
 , Typ_Hierarchy.value(&apos;&#47;Typ[1]&#47;Lvl[14]&apos;,&apos;varchar(100)&apos;) AS [Typ14]
 
 FROM cte)
SELECT
  OE1
, COALESCE(OE2, OE1) AS OE2
