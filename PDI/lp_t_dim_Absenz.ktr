  <step>
    <name>Absenzen lesen</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Pentaho_Staging</connection>
    <sql>SELECT DISTINCT	
	ABZ.COPKEY
	, ABZ.ABCKEY
	, ABC.ABCCDE
	, ABC.ABCDESC
	, ABZ.ABTKEY
	, ABT.ABTCDE
	, ABT.ABTDESC
	, ABT.ABTUNIT
	, ABT.ABTAUTH
	, ABT.ABTCALTI
	, CASE	
		WHEN ABT.ABTCALTI = &apos;W&apos; THEN &apos;Arbeitstage&apos;
		WHEN ABT.ABTCALTI = &apos;C&apos; THEN &apos;Kalendertage&apos;
		WHEN ABT.ABTCALTI = &apos;H&apos; THEN &apos;Stunden&apos;
		WHEN ABT.ABTCALTI = &apos;S&apos; THEN &apos;Statistiktage&apos;
		Else &apos;&apos;
		END
		AS AbsenzArtInternKalenderBez
	, ABT.ABTCALTE
	, CASE 
		WHEN ABT.ABTCALTE = &apos;W&apos; THEN &apos;Arbeitstage&apos;
		WHEN ABT.ABTCALTE = &apos;C&apos; THEN &apos;Kalendertage&apos;
		WHEN ABT.ABTCALTE = &apos;H&apos; THEN &apos;Stunden&apos;
		WHEN ABT.ABTCALTE = &apos;S&apos; THEN &apos;Statistiktage&apos;
		Else &apos;&apos;
		END
		AS AbsenzArtExternKalenderBez
	, ABT.ABTTXTI
	, ABT.ABTSTAT1
	, ABT.ABTSTAT2
	, ABT.ABTSTAT3
	, ABT.ABTUNSAL
	, ABT.ABTACT
	, ABT.ABTCOE
	, ABT.ABTBALF
	, ABT.ABTWP
	, ABT.HR51aA
	, ABT.HR51xA
	, ABT.HR51yA
	, ABT.MASKEY


	, ABZ.HRnjcn4A AS idInternStatus
	, ABS_INTERN.ABSCDE AS InternStatusCode
	, ABS_INTERN.ABSDESC AS InternStatusBez
	, ABZ.HRnjcn5A AS idExternStatus
	, ABS_EXTERN.ABSCDE AS ExternStatusCode
	, ABS_EXTERN.ABSDESC AS ExternStatusBez

FROM lp_t_stage_HRMABZ ABZ 
LEFT OUTER JOIN lp_t_stage_HRMABC ABC ON ABZ.ABCKEY = ABC.ABCKEY
LEFT OUTER JOIN lp_t_stage_HRMABT ABT ON ABZ.ABTKEY = ABT.ABTKEY
LEFT OUTER JOIN lp_t_stage_HRMABE ABE ON ABZ.ABEKEY = ABE.ABEKEY
LEFT OUTER JOIN lp_t_stage_HRMABS ABS_INTERN ON ABZ.HRnjcn4A = ABS_INTERN.ABSKEY
LEFT OUTER JOIN lp_t_stage_HRMABS ABS_EXTERN ON ABZ.HRnjcn5A = ABS_EXTERN.ABSKEY
ORDER BY ABZ.COPKEY, ABZ.ABCKEY, ABZ.ABTKEY

</sql>
