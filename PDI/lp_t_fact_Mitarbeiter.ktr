, COHPOSLY
, COHPOS
, COHPOSY
, COHAGE
, COHAGEY
, COHCHIEP
, COHPAYAL
, COHSTRNA
, COHSTRNB
, COHSTRNC
, CONSTRND
, COHSTRNE
, COHCOSTC
, COHEMPPC
, COHCHBCA
, COHCHSTA
, COHCHCOU
, COHFICO
, COHCHCOM
, COH.FIPCDE
, COH.COMPANY
, SSEKEY
, UVIID
, BRACODE
, COHTAXNO
, ASN.OUNKEY
, CONVERT(DECIMAL(10,2),ROUND(20*COALESCE(SAL1009.SALAMOUN, SAL1009.SALAMOUN,0)&#47;13.0,0)&#47;20) AS Monatslohn
, COALESCE(SAL1009.SALAMOUN, SAL1009.SALAMOUN,0) AS Jahreslohn
, Periode
, DatumVon
, DatumBis


 &#47;* Achtung bei Case Anpassung -&gt; Identischer Case beim JOIN auf ASN *&#47;
, CASE 
--Falls wir im aktuellen Monat stehen wollen wir dass aktuelle Datum und nicht der letzte des Monats weil die Belegung sich bis Ende Monat allenfalls ändert
	WHEN Periode = CAC.CACYEAR * 100 + CAC.CACMONTH AND Periode = Year(GETDATE())*100+MONTH(GETDATE()) THEN DATEADD(dd, DATEDIFF(d, 0, GETDATE()), 0)
--Fall das Austrittsdatum in der jeweiligen Periode liegt, Belegung per Austrittsdatum joinen (da möglicherweise Mitte Monat)
	WHEN Periode = YEAR(ELHLEAD)*100+MONTH(ELHLEAD) THEN ELHLEAD
	ELSE DatumBis
	END AS Stichtag


FROM lp_t_stage_HRMCOH COH

LEFT OUTER JOIN lp_t_stage_HRMELH ELH ON
	COH.EMPKEY = ELH.EMPKEY AND
	COH.CONNUMB = ELH.CONNUMB AND
	COH.Periode BETWEEN ELH.PeriodeVon AND ELH.PeriodeBis

LEFT OUTER JOIN lp_t_stage_HRMCAC CAC ON COH.COHCALCC = CAC.CACCODE

LEFT OUTER JOIN lp_t_stage_HRMASN ASN ON
	COH.EMPKEY = ASN.EMPKEY AND
	COH.CONNUMB = ASN.CONNUMB AND
	ASN.ASNMAIN = &apos;Y&apos; AND
		CASE 
			--Falls wir im aktuellen Monat stehen wollen wir dass aktuelle Datum und nicht der letzte des Monats weil die Belegung sich bis Ende Monat allenfalls ändert
			WHEN Periode = CAC.CACYEAR * 100 + CAC.CACMONTH AND Periode = Year(GETDATE())*100+MONTH(GETDATE()) THEN DATEADD(dd, DATEDIFF(d, 0, GETDATE()), 0)
			--Fall das Austrittsdatum in der jeweiligen Periode liegt, Belegung per Austrittsdatum joinen (da möglicherweise Mitte Monat)
			WHEN Periode = YEAR(ELHLEAD)*100+MONTH(ELHLEAD) THEN ELHLEAD
			ELSE DatumBis
		END 
	 BETWEEN ASNFROM AND ASNUNTIL
		 
LEFT OUTER JOIN lp_t_stage_HRMSAL SAL1009 ON
	COH.EMPKEY = SAL1009.EMPKEY AND
	COH.CONNUMB = SAL1009.CONNUMB AND
	COH.Periode	BETWEEN SAL1009.PeriodeVon AND SAL1009.PeriodeBis AND
	SAL1009.SATKEY = &apos;1009&apos; AND
	SAL1009.SALPLAN = &apos;O&apos;
	
INNER JOIN lp_t_stage_HRMCOT COT ON
	COH.COHCONTY = COT.COTKEY






ORDER BY COH.EMPKEY, COH.CONNUMB, COHPERY, COHPERM





</sql>
