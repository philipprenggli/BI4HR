  <step>
    <name>Letzer Tag pro Monat (Vergangenheit&#47;Zukunft)</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
