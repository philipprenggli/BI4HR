    <connection>Pentaho_DWH</connection>
    <sql>-- Vergangenheit
SELECT * FROM
(
SELECT	MAX(Datum) AS Datum
FROM	lp_t_dim_Zeit
WHERE	Datum &gt;= CONVERT(DATETIME,&apos;${XHRM_ORG_DATENAB_DATUM}&apos;,104)
	AND	Jahr*100+Monat &lt; YEAR(GETDATE())*100+MONTH(GETDATE())
GROUP BY Jahr, Monat) A

UNION 

-- Zukunft
SELECT * FROM
(
SELECT	MAX(Datum) AS Datum
FROM	lp_t_dim_Zeit
WHERE	Datum &gt;= CONVERT(DATETIME,&apos;${XHRM_ORG_DATENAB_DATUM}&apos;,104)
	AND	Jahr*100+Monat &gt; YEAR(GETDATE())*100+MONTH(GETDATE())
	AND Jahr*100+Monat &lt;= YEAR(DATEADD(YEAR,1,GETDATE()))*100+MONTH(DATEADD(YEAR,1,GETDATE()))
GROUP BY Jahr, Monat) A
ORDER BY Datum</sql>
