  <step>
    <name>Absenzen</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Pentaho_Staging</connection>
    <sql>SELECT 
	
	ABZ.COPKEY
	, ABZ.EMPKEY
	, ABZ.ABZKEY
	, ABZ.ABZDATF AS AbsenzDatumVon
	, CASE
		WHEN ABZ.HR5hoA = &apos;&apos; THEN 1
		ELSE ABZ.HR5hoA
		END AS AbsenzDatumVonStatusEnumerator
	, ABZ.ABZDATT AS AbsenzDatumBis
	, CASE 
		WHEN ABZ.HR5hpA = &apos;&apos; THEN 1
		ELSE ABZ.HR5hpA
		END AS AbsenzDatumBisStatusEnumerator
	, ABZ.ABCKEY
	, ABZ.ABTKey
	, ABZ.HRnjcn4A
	, ABZ.HRnjcn5A
	, Zeit.Datum AS Stichtag
	, Zeit.idZeit
	, Zeit.TagWoche
	, CASE
		WHEN HOLTYPE IS NULL THEN 0
		WHEN HOLTYPE = 1 THEN 1 --Ganzer Tag Frei
		WHEN HOLTYPE = 2 OR HOLTYPE = 3 THEN 0.5 --Vormittag oder Nachmittag Frei
		WHEN HOLTYPE = 4 THEN 0 --Brückentage
		END AS Feiertage

	, ISNULL(CPA_WORKDAY.CPAPARV, ABP.WRKKEY) AS WORKKEY
	, ISNULL(CPA_CALENDAR.CPAPARV, ABP.CALKEY) AS CALKEY
		
	, CASE 
		WHEN Zeit.TagWoche = 2 THEN --Monday
			CASE 
				WHEN WRK.WRKMON = &apos;&apos; THEN 0
				WHEN WRK.WRKMON = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKMON = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKMON = 3 THEN 0.5 --Nachmittag
			END
		WHEN Zeit.TagWoche = 3 THEN --Tuesday
			CASE 
				WHEN WRK.WRKTUE = &apos;&apos; THEN 0
				WHEN WRK.WRKTUE = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKTUE = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKTUE = 3 THEN 0.5 --Nachmittag
			END
		WHEN Zeit.TagWoche = 4 THEN --Wednesday
			CASE 
				WHEN WRK.WRKWED = &apos;&apos; THEN 0
				WHEN WRK.WRKWED = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKWED = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKWED = 3 THEN 0.5 --Nachmittag
				END
		WHEN Zeit.TagWoche = 5 THEN --Thurdsay
			CASE 
				WHEN WRK.WRKTHU = &apos;&apos; THEN 0
				WHEN WRK.WRKTHU = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKTHU = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKTHU = 3 THEN 0.5 --Nachmittag
				END
		WHEN Zeit.TagWoche = 6 THEN --Friday
			CASE 
				WHEN WRK.WRKFRI = &apos;&apos; THEN 0
				WHEN WRK.WRKFRI = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKFRI = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKFRI = 3 THEN 0.5 --Nachmittag
				END
		WHEN Zeit.TagWoche = 7 THEN --Saturday
			CASE 
				WHEN WRK.WRKSAT = &apos;&apos; THEN 0
				WHEN WRK.WRKSAT = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKSAT = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKSAT = 3 THEN 0.5 --Nachmittag
				END
		WHEN Zeit.TagWoche = 1 THEN --Sunday
			CASE 
				WHEN WRK.WRKSUN = &apos;&apos; THEN 0
				WHEN WRK.WRKSUN = 1 THEN 1 --ganzer Tag 
				WHEN WRK.WRKSUN = 2 THEN 0.5 --Vormittag
				WHEN WRK.WRKSUN = 3 THEN 0.5 --Nachmittag
				END
		END AS Arbeitstage
	, ABZ.ABZRATI AS RatioInternal
FROM lp_t_stage_HRMABZ ABZ
LEFT OUTER JOIN lp_t_stage_HRMABC ABC ON ABZ.ABCKEY = ABC.ABCKEY

CROSS JOIN Pentaho_Demo_DWH.dbo.lp_t_dim_Zeit Zeit 

LEFT OUTER JOIN lp_t_stage_HRMCPA CPA_WORKDAY ON ABZ.EMPKEY = CPA_WORKDAY.EMPKEY AND CPA_WORKDAY.CPAPART = &apos;WORKDAY&apos;
LEFT OUTER JOIN lp_t_stage_HRMCPA CPA_CALENDAR ON ABZ.EMPKEY = CPA_CALENDAR.EMPKEY AND CPA_CALENDAR.CPAPART = &apos;CALENDAR&apos;
LEFT OUTER JOIN lp_t_stage_HRMABP ABP ON ABZ.COPKEY = ABP.COPKEY
--Falls individueller Kalender hinterlegt diesen nehmen, ansonsten den Firmenübergreifenden
LEFT OUTER JOIN lp_t_stage_HRMHOL HOL ON ISNULL(CPA_CALENDAR.CPAPARV, ABP.CALKEY) = HOL.CALKEY AND Zeit.Datum = HOL.HOLDAT
--Falls individuelle Arbeitstag-Woche hinterlegt diesen nehmen, ansonsten den Firmenübegreifenden
LEFT OUTER JOIN lp_t_stage_HRMWRK WRK ON ISNULL(CPA_WORKDAY.CPAPARV, ABP.WRKKEY) = WRK.WRKKEY AND ABZ.COPKEY = WRK.COPKEY


WHERE 
	Zeit.Datum &gt;= ABZ.ABZDATF AND Zeit.Datum &lt;= ABZ.ABZDATT







	






	




</sql>
