    <connection>Pentaho_DWH</connection>
    <sql>--Quelle ist die Zeit-Dimension. Muss also vorher bereits erstellt sein
SELECT RANK() OVER (ORDER BY Jahr,Monat ASC) AS Rang, *  FROM 
(
	SELECT  DISTINCT 
	CAST(CAST(Jahr as varchar) + RIGHT(&apos;0&apos; + CAST(Monat AS VARCHAR),2) AS INT) AS Periode, Jahr, Monat, MonatName, MonatNameKurz, Quartal
	from lp_t_dim_Zeit
) Perioden ORDER BY Periode</sql>
