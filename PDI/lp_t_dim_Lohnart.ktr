    <sql>SELECT
  SAT.COPKEY
, SATKEY
, SATDESC
, SATCALS
, SATRNDCD
, SATRNDM
, SATPGRP
, SATPSN
, SATFXDRT
, SATFCT
, SATFTC
, SATFOC
, SATPRF
, SATPTP
, SATPTT
, SATQTYF
, SATAMTF
, SATACD
, SATADJE
, SATAPK1
, SATAPK2
, SATACTL
, SATDPS
, SATRVS
, SATPRTA
, SATCONV
, SATTRSF
, SATBSAL
, SATRPV
, SATRATC
, SATSNGI
, SATCDQ
, SATCDR
, SATCDA
, SATCDP
, SATCDB
, SATMANI
, SATCFO
, SATMAS
, SATRTSAT
, SATLOJAN
, SATLOFEB
, SATLOMAR
, SATLOAPR
, SATLOMAY
, SATLOJUN
, SATLOJUL
, SATLOAUG
, SATLOSEP
, SATLOOCT
, SATLONOV
, SATLODEC
, SATSDESC
, SATRCTYP
, SATIGRCT
, SATRCAMT
, SATRCPCT
, SATRCQTY
, SATRCRAT
, SATBFSRL
, SATBFSRS

, PGR.PGRCODE
, PGR.PGRLDESC
, PGR.PGRSDESC

, CAC.CACDATFR
, CAC.CACDATTI

FROM lp_t_stage_HRMSAT SAT
INNER JOIN lp_t_stage_HRMCAC CAC ON
	CAC.CACCODE = &apos;${XHRM_CACCODE}&apos;
INNER JOIN lp_t_stage_HRMPGR PGR ON 
	SAT.COPKEY = PGR.COPKEY AND
	SAT.SATPGRP = PGR.PGRKEY
	
ORDER BY COPKEY,SATKEY
</sql>
