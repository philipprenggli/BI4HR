    <connection>Pentaho_Staging</connection>
    <sql>--StandardKonfiguration Kartesisches Product mit EMPKEYs die nicht eigene Konfig haben
SELECT 
	EMPKEYS.EMPKEY AS EMPKEY
	, WRK.WRKKey
	, CASE WHEN WRK.WRKMON = 1 THEN 2 ELSE 0 END AS We1
	, CASE WHEN WRK.WRKTUE = 1 THEN 3 ELSE 0 END AS We2
	, CASE WHEN WRK.WRKWED = 1 THEN 4 ELSE 0 END AS We3
	, CASE WHEN WRK.WRKTHU = 1 THEN 5 ELSE 0 END AS We4
	, CASE WHEN WRK.WRKFRI = 1 THEN 6 ELSE 0 END AS We5
	, CASE WHEN WRK.WRKSAT = 1 THEN 7 ELSE 0 END AS We6
	, CASE WHEN WRK.WRKSUN = 1 THEN 1 ELSE 0 END AS We7
	 FROM lp_t_stage_HRMABP ABP
LEFT OUTER JOIN lp_t_stage_HRMWRK WRK ON ABP.WRKKey = WRK.WRKKEY
CROSS JOIN (SELECT DISTINCT EMPKEY FROM lp_t_stage_HRMABZ  WHERE EMPKEY NOT IN (SELECT EMPKEY FROM lp_t_stage_HRMCPA WHERE CPAPART = &apos;Workday&apos;)) EMPKEYS

UNION ALL
--Mitarbeiter mit eigener Konfig
SELECT 
	  CPA.EMPKey
	, WRK.WRKKey
	, CASE WHEN WRK.WRKMON = 1 THEN 2 ELSE 0 END AS We1
	, CASE WHEN WRK.WRKTUE = 1 THEN 3 ELSE 0 END AS We2
	, CASE WHEN WRK.WRKWED = 1 THEN 4 ELSE 0 END AS We3
	, CASE WHEN WRK.WRKTHU = 1 THEN 5 ELSE 0 END AS We4
	, CASE WHEN WRK.WRKFRI = 1 THEN 6 ELSE 0 END AS We5
	, CASE WHEN WRK.WRKSAT = 1 THEN 7 ELSE 0 END AS We6
	, CASE WHEN WRK.WRKSUN = 1 THEN 1 ELSE 0 END AS We7

FROM lp_t_stage_HRMCPA CPA
LEFT OUTER JOIN lp_t_stage_HRMWRK WRK ON  CPA.CPAPARV = WRK.WRKKEY
WHERE CPA.CPAPART = &apos;Workday&apos;



</sql>
