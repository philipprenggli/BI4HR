    <sql>SELECT 
	 ABZ.EMPKEY
	, ABZ.ABZKEY
	, ABZ.ABZDATF AS AbsenzDatumVon
	, ABZ.HR5hoA AS AbsenzDatumVonStatusEnumerator
	, ABZ.ABZDATT AS AbsenzDatumBis
