    <sql>SELECT
  EMPKEY
, CONNUMB
, COPKEY
, SATKEY
, SALYEAR
, SALMONTH
, SALLINE
, PAYCACNR
, PAYQUANT
, PAYRATE
, PAYAMT
, PAYPCT
, PAYBASE
, PAYTEXT
, PAYLNGID
, PAYSHOW
, PAYPRTGR
, PAYPRTSQ
, PAYMENTY
, PAYDATE
, PAYTIME
, PAYUSER
, PAYFICO
, COMPANY
, PAYRYEAR
, PAYRMTH
, Periode
, DatumVon
, DatumBis
, CASE 
	WHEN DatumBis &lt;= DATEADD(dd, DATEDIFF(d, 0, GETDATE()), 0) THEN DatumBis
	ELSE DATEADD(dd, DATEDIFF(d, 0, GETDATE()), 0)
  END AS Stichtag
FROM lp_t_stage_HRMPAY
WHERE (SALYEAR*100+SALMONTH) &gt;= ${XHRM_PAY_DATENAB_PERIODE}



</sql>
