		END 
		AS StatusDatumVonBez
, ABZ.ABZTIMF
, ABZ.ABZDATT
, ABZ.HR5hpA
, CASE	
		WHEN ABZ.HR5hpA = &apos;1&apos; THEN &apos;ganzer Tag&apos;
		WHEN ABZ.HR5hpA = &apos;2&apos; THEN &apos;Vormittag&apos;
		WHEN ABZ.HR5hpA = &apos;3&apos; THEN &apos;Nachmittag&apos;
		ELSE &apos;&apos;
		END 
		AS StatusDatumBisBez
, ABZ.ABZTIMT
, ABZ.ABZRAT
, ABZ.HRnjcn1A
, ABZ.HRnjcn2A
, ABZ.HRnjcn3A
, ABZ.ABZDAYI
, ABZ.ABZHRSI
, ABZ.ABZDAYIC
, ABZ.ABZRATI
, ABZ.ABZDAYE
, ABZ.ABZDAYEC
, ABZ.ABZRATE
, ABZ.ABZTXT
, ABZ.ABZACT
, ABZ.ABZPOST
, ABZ.ABZQTY1
, ABZ.ABZQTY2
, ABZ.ABZLDLT
, ABZ.ABZTECH
, ABZ.ABZTAG
, ABZ.ABZSTD
, ABZ.COPKEY
, ABZ.ABCKEY
, ABZ.ABTKEY
, ABE.ABECDE
, ABE.ABEDESC
, ABZ.HRnjcn4A
, ABZ.HRnjcn5A	
, ASN.OUNKEY
, ABZDATF AS Stichtag
FROM lp_t_stage_HRMABZ ABZ
LEFT OUTER JOIN lp_t_stage_HRMABE ABE ON ABZ.ABEKEY = ABE.ABEKEY
LEFT OUTER JOIN lp_t_stage_HRMASN ASN ON 
	ABZ.EMPKEY = ASN.EMPKEY AND
	ABZ.CONNUMB = ASN.CONNUMB AND
	ASN.ASNMAIN = &apos;Y&apos; AND
	((ABZDATF BETWEEN ASNFROM AND ASNUNTIL)  OR (ABZDATT BETWEEN ASNFROM AND ASNUNTIL) OR (ASNFROM &gt;= ABZDATF AND ASNUNTIL &lt;= ABZDATT)  )
ORDER BY ABZ.EMPKEY, ABZ.CONNUMB
</sql>
