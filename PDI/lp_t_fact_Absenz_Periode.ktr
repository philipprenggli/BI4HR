, ABZ.CONNUMB	
, ABZ.ABZKEY
, ABZ.ABZDATF
, ABZ.HR5hoA
, CASE	
		WHEN ABZ.HR5hoA = &apos;1&apos; THEN &apos;ganzer Tag&apos;
		WHEN ABZ.HR5hoA = &apos;2&apos; THEN &apos;Vormittag&apos;
		WHEN ABZ.HR5hoA = &apos;3&apos; THEN &apos;Nachmittag&apos;
