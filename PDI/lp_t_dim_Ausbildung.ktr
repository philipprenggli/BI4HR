  <step>
    <name>Ausbildungen lesen</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>Pentaho_Staging</connection>
    <sql>SELECT DISTINCT
	  EVT.EVGKEY
	, EVGNBR AS Kursnummer
	, EVGDESC1 AS Kursbezeichnung1
	, EVGDESC2 AS Kursbezeichnung2

	--, EVGPRC AS EVG_Price
	--, EVGPRCP AS EVG_PricePerson
	--, EVGPRCC AS EVG_PriceCompany
	--, EVGCUR AS EVG_Currency
	
	, EMC.EMPKEY AS Kursverantworlicher_EMPKEY
	--, EVGADMC AS EVG_AdministratorCorrespondent
	, EVGSTS AS KursStatusEnumerator
	, CASE	
		WHEN EVGSTS = 1 THEN &apos;offen&apos;
		WHEN EVGSTS = 3 THEN &apos;geschlossen&apos;
		WHEN EVGSTS = 2 THEN &apos;provisorisch&apos;
		Else &apos;&apos;
		END
		AS KursStatusBezeichnung
	, EVGWSTS AS KursWebStatusEnumerator
	, CASE	
		WHEN EVGWSTS = &apos;A&apos; THEN &apos;nur anzeigen&apos;
		WHEN EVGWSTS = &apos;W&apos; THEN &apos;ändern&apos;
		Else &apos;keine Angabe&apos;
		END
		AS KursWebStatusBezeichnung
	, EVGSECI AS KursGruppe
	, EVGCOMN AS UnternehmenNr
	, COM.COMDESC AS Unternehmen
	--, EVGARTS AS EVG_ArticleSurrogate Vorläufig nicht implementieren
	, EVGCC AS KSTEnumerator
	, COSTCENTER.CX1jrA AS KSTBezKurz
	, COSTCENTER.CX2zmA AS KSTBez
	, EVG.EDDKEY AS BereichEnumerator
	, EDD.EDDCODE AS BereichCode
	, EDD.EDDDESC AS BereichBezeichnung
	, EVGTYPE AS KursTyp
	, EVG.EGSKEY AS Status2Enumerator
	, EGS.EGSCODE AS Status2Code
	, EGS.EGSDESC AS Status2Bezeichnung
	

	, EVT.EVTKEY AS EVTKEY
	, EVTNBR AS Anlassnummer
	, EVTYEAR AS Anlassjahr

	--, EVTMAN AS EVT_ManagerMaster
	--, EVTMANC AS EVT_ManagerCorrespondent
	--, EVTTEACH AS EVT_Teacher
	
	, EVTDAYS AS AnlassKurstage
	
	--, EVTEOR AS EVT_EndOfRegistration

	, EVTBEGIN AS AnlassStartdatum
	, EVTEND AS AnlassEnddatum
	, EVTMINP AS AnlassMinTeilnehmer
	, EVTMAXP AS AnlassMaxTeilnehmer
	, EVTOCC AS AnlassBelegstatusEnumerator
	, CASE	
		WHEN EVTOCC = 1 THEN &apos;freie Plätze&apos;
		WHEN EVTOCC = 2 THEN &apos;belegt&apos;
		WHEN EVTOCC = 3 THEN &apos;überbelegt&apos;
		END
		AS AnlassBelegstatusBezeichnung
	, EVTLCK AS AnlassGesperrt
	, EVTSTS AS AnlassStatusEnumerator
	, CASE	
		WHEN EVTSTS = 1 THEN &apos;offen&apos;
		WHEN EVTSTS = 3 THEN &apos;geschlossen&apos;
		WHEN EVTSTS = 2 THEN &apos;provisorisch&apos;
		Else &apos;&apos;
		END
		AS AnlassStatusBezeichnung
	, EVTWSTS AS AnlassWebStatusEnumerator
	, CASE	
		WHEN EVTWSTS = &apos;A&apos; THEN &apos;nur anzeigen&apos;
		WHEN EVTWSTS = &apos;W&apos; THEN &apos;ändern&apos;
		Else &apos;keine Angabe&apos;
		END
		AS AnlassWebStatusBezeichnung
	, EVTLNG AS KursspracheCode
	, CASE	
		WHEN EVTLNG = &apos;DE&apos; THEN &apos;Deutsch&apos;
		WHEN EVTLNG = &apos;FR&apos; THEN &apos;Français&apos;
		WHEN EVTLNG = &apos;IT&apos; THEN &apos;Italiano&apos;
		WHEN EVTLNG = &apos;EN&apos; THEN &apos;English&apos;
		Else &apos;&apos;
		END
		AS KursspracheBezeichnung


	--, RETKEY AS RETKEY
	--, RESKEY AS RESKEY

	, REG.REGSTAT AS TeilnahmestatusEnumerator
	, CASE 
		WHEN REG.REGSTAT = &apos;0&apos; THEN &apos;Optioniert&apos;
		WHEN REG.REGSTAT = &apos;1&apos; THEN &apos;Angemeldet&apos;
		WHEN REG.REGSTAT = &apos;2&apos; THEN &apos;Warteliste&apos;
		WHEN REG.REGSTAT = &apos;3&apos; THEN &apos;Bestätigt&apos;
		WHEN REG.REGSTAT = &apos;4&apos; THEN &apos;Abgesagt&apos;
		WHEN REG.REGSTAT = &apos;5&apos; THEN &apos;Eingeladen&apos;
		WHEN REG.REGSTAT = &apos;6&apos; THEN &apos;Abgemeldet ohne Verrechnung&apos;
		WHEN REG.REGSTAT = &apos;7&apos; THEN &apos;Abgemeldet mit Verrechnung&apos;
		WHEN REG.REGSTAT = &apos;8&apos; THEN &apos;Abgeschlossen&apos;
		WHEN REG.REGSTAT = &apos;9&apos; THEN &apos;Gelöscht&apos;
		WHEN REG.REGSTAT = &apos;C&apos; THEN &apos;Umgebucht&apos;
		ELSE &apos;&apos;
		END AS Teilnahmestatus
	, REG.RUSKEY AS Teilnahmestatus2Enumerator
	, RUS.RUSCODE AS Teilnahmestatus2Code
	, RUS.RUSDESC AS Teilnahmestatus2Bezeichnung
	, REG.REGHSTS AS HotelstatusEnumerator
	, CASE 
		WHEN REG.REGHSTS = 0 THEN &apos;Nicht reserviert&apos;
		WHEN REG.REGHSTS = 1 THEN &apos;Reserviert&apos;
		WHEN REG.REGHSTS = 2 THEN &apos;angefordert&apos;
		ELSE &apos;&apos;
		END AS Hotelstatus


 FROM lp_t_stage_HRMEVT EVT
--LEFT OUTER JOIN lp_t_stage_HRMEVT EVT ON REG.EVTKEY = EVT.EVTKEY
LEFT OUTER JOIN lp_t_stage_HRMEVG EVG ON EVT.EVGKEY = EVG.EVGKEY
LEFT OUTER JOIN lp_t_stage_HRMEMC EMC ON EVG.EVGADM = EMC.PADMACON
LEFT OUTER JOIN lp_t_stage_HRMCOM COM ON EVG.EVGCOMN = COM.COMPANY
LEFT OUTER JOIN lp_t_stage_CX4CPT COSTCENTER ON COSTCENTER.CX2ibA = EVG.EVGCC
LEFT OUTER JOIN lp_t_stage_HRMEGS EGS ON EVG.EGSKEY = EGS.EGSKEY
LEFT OUTER JOIN lp_t_stage_HRMEDD EDD ON EVG.EDDKEY = EDD.EDDKEY
LEFT OUTER JOIN lp_t_stage_HRMREG REG ON EVT.EVTKEY = REG.EVTKEY 
LEFT OUTER JOIN lp_t_stage_HRMRUS RUS ON REG.RUSKEY = RUS.RUSKEY</sql>
