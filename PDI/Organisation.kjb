    <entry>
      <name>OE Darstellungsvariante 2 - Teil 2</name>
      <description/>
      <type>SQL</type>
      <sql>with tmp as (
    select *, idCounter=ROW_NUMBER() over (partition by t.idoe, t.stichtag order by sort)
    from lp_t_tmp_Organisation t
    outer apply
    (
        select 1, t.OE1var2 where t.OE1var2 is not null union all
        select 2, t.OE2var2 where t.OE2var2 is not null union all
       select 3, t.OE3var2 where t.OE3var2 is not null union all
       select 4, t.OE4var2 where t.OE4var2 is not null union all
       select 5, t.OE5var2 where t.OE5var2 is not null union all
       select 6, t.OE6var2 where t.OE6var2 is not null union all
       select 7, t.OE7var2 where t.OE7var2 is not null union all
       select 8, t.OE8var2 where t.OE8var2 is not null union all
       select 9, t.OE9var2 where t.OE9var2 is not null union all
       select 10, t.OE10var2 where t.OE10var2 is not null union all
       select 11, t.OE11var2 where t.OE11var2 is not null union all
       select 12, t.OE12var2 where t.OE12var2 is not null union all
       select 13, t.OE13var2 where t.OE13var2 is not null union all
       select 14, t.OE14var2 where t.OE14var2 is not null
           
    ) u(sort, OE)
)


UPDATE lp_t_tmp_Organisation
set OE1Var2 = t1.OE,
    OE2Var2 = t2.OE,
    OE3Var2 = t3.OE,
    OE4Var2 = t4.OE,
    OE5Var2 = t5.OE,
    OE6Var2 = t6.OE,
    OE7Var2 = t7.OE,
    OE8Var2 = t8.OE,
    OE9Var2 = t9.OE,
    OE10Var2 = t10.OE,
    OE11Var2 = t11.OE,
    OE12Var2 = t12.OE,
    OE13Var2 = t13.OE,
    OE14Var2 = t14.OE
from lp_t_tmp_Organisation t0
left join tmp t1 on t1.idoe = t0.idOE and t1.Stichtag = t0.Stichtag and t1.idCounter=1
left join tmp t2 on t2.idoe = t0.idOE and t2.Stichtag = t0.Stichtag and t2.idCounter=2
left join tmp t3 on t3.idoe = t0.idOE and t3.Stichtag = t0.Stichtag and t3.idCounter=3
left join tmp t4 on t4.idoe = t0.idOE and t4.Stichtag = t0.Stichtag and t4.idCounter=4
left join tmp t5 on t5.idoe = t0.idOE and t5.Stichtag = t0.Stichtag and t5.idCounter=5
left join tmp t6 on t6.idoe = t0.idOE and t6.Stichtag = t0.Stichtag and t6.idCounter=6
left join tmp t7 on t7.idoe = t0.idOE and t7.Stichtag = t0.Stichtag and t7.idCounter=7
left join tmp t8 on t8.idoe = t0.idOE and t8.Stichtag = t0.Stichtag and t8.idCounter=8
left join tmp t9 on t9.idoe = t0.idOE and t9.Stichtag = t0.Stichtag and t9.idCounter=9
left join tmp t10 on t10.idoe = t0.idOE and t10.Stichtag = t0.Stichtag and t10.idCounter=10
left join tmp t11 on t11.idoe = t0.idOE and t11.Stichtag = t0.Stichtag and t11.idCounter=11
left join tmp t12 on t12.idoe = t0.idOE and t12.Stichtag = t0.Stichtag and t12.idCounter=12
left join tmp t13 on t13.idoe = t0.idOE and t13.Stichtag = t0.Stichtag and t13.idCounter=13
left join tmp t14 on t14.idoe = t0.idOE and t14.Stichtag = t0.Stichtag and t14.idCounter=14
</sql>
