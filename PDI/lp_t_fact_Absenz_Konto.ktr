    <connection>Pentaho_Staging</connection>
    <sql>
SELECT 
	  ACC.COPKEY
	, ACC.ACCKEY AS AbsenzkontoEnumerator
	, ACC.ACCCDE AS AbsenzkontoCode
	, ACC.ACCDESC AS AbsenzkontoBezeichnung
	, ABG.ABGKEY AS AbsenzgruppeEnumerator
	, ABG.ABGCDE AS AbsenzgruppeCode
	, ABG.ABGDESC AS AbsenzgruppeBezeichnung
	, ABG.ABGTOT AS AbsenzgruppeTotalzeile
	, ACG.ACGSORT AS AbsenzgruppePosition
	, ABZ.EMPKEY
	, ABZ.CONNUMB	
	, ABZ.ABZKEY AS AbsenzEnumerator
	, ABZ.ABZDATF AS AbsenzDatumVon
	, ABZ.ABZDATT AS AbsenzDatumBis
	, ABZ.ABZTXT AS AbsenzText
	, CASE 
		WHEN AGT.AGTSIGN = &apos;+&apos; THEN ABZ.ABZTAG
		ELSE ABZ.ABZTAG * (-1)
		END AS AbsenzTageKonto

	, CASE 
		WHEN AGT.AGTSIGN = &apos;+&apos; THEN ABZ.ABZSTD
		ELSE ABZ.ABZSTD * (-1)
		END AS AbsenzStundenKonto
	, ABZ.ABCKEY
	, ABZ.ABTKEY
	, ABE.ABEKEY AS AbsenzEreignisEnumerator
	, ABE.ABECDE AS AbsenzEreignisCode
	, ABE.ABEDESC AS AbsenzEreignisBezeichnung
	, ABZ.HRnjcn4A
	, ABZ.HRnjcn5A	
	, ABZDATF AS Stichtag


FROM lp_t_stage_HRMACG ACG
